<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CAT-APP-PROJECT</display-name>
   <servlet>
    <servlet-name>CoverServlet</servlet-name>
    <servlet-class>com.catapp.servlet.CoverServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.catapp.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>NewUserServlet</display-name>
    <servlet-name>NewUserServlet</servlet-name>
    <servlet-class>com.catapp.servlet.NewUserServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>UploadServlet</display-name>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>com.catapp.servlet.UploadServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>SaveFileFormServlet</display-name>
    <servlet-name>FileSave</servlet-name>
    <servlet-class>com.catapp.servlet.SaveFileFormServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>UserHomeServlet</display-name>
    <servlet-name>UserHomeServlet</servlet-name>
    <servlet-class>com.catapp.servlet.UserHomeServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>ViewDownloadServlet</display-name>
    <servlet-name>ViewDownloadServlet</servlet-name>
    <servlet-class>com.catapp.servlet.ViewDownloadServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>UserManagementServlet</display-name>
    <servlet-name>UserManagementServlet</servlet-name>
    <servlet-class>com.catapp.servlet.UserManagementServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet>
    <display-name>ManageActionServlet</display-name>
    <servlet-name>ManageActionServlet</servlet-name>
    <servlet-class>com.catapp.servlet.ManageActionServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/Admin</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>FileUploadServlet</display-name>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>com.catapp.servlet.UploadServlet</servlet-class>
    <enabled>true</enabled>
  </servlet>
  
    
   <servlet-mapping>
    <servlet-name>CoverServlet</servlet-name>
    <url-pattern>/Cover</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewUserServlet</servlet-name>
    <url-pattern>/NewUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserHomeServlet</servlet-name>
    <url-pattern>/UserHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/Upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileSave</servlet-name>
    <url-pattern>/FileSave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewDownloadServlet</servlet-name>
    <url-pattern>/ViewDownload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/fileupload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserManagementServlet</servlet-name>
    <url-pattern>/UserManagement</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageActionServlet</servlet-name>
    <url-pattern>/ManageAction</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/WEB-INF/Cover.jsp</welcome-file>
  </welcome-file-list>
</web-app>